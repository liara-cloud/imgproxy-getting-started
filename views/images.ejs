<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Uploaded Images</title>
</head>
<body>
    <h1>Uploaded Images</h1>
    <% images.forEach(function(image) { %>
        <img src="/public/uploads/<%= image %>" alt="<%= image %>">
    <% }); %>
</body>
</html>
